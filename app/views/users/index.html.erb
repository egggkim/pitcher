<h1>All users</h1>
<p><%= link_to "Sign Up", new_user_path %></p>

<% if logged_in? %>
<%= link_to 'Log Out', logout_path, method: :delete %>
<br>
Logged in as: <%= current_user.first_name %>
<% else %>
  <%= link_to 'Log In', login_path %>
<% end %>

<ul>
  <% @users.each do |user| %>
    <li>
      <%= link_to user.first_name, user %>
    </li>
  <% end %>
</ul>

